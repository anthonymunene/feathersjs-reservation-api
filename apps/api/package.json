{
  "name": "api",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Anthony Munene"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 20.9.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "postgresql",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js",
    "lib/utils/s3/lib/*.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "npm run bundle:client && infisical run --env=dev --path=/Apps/reservation-apps/api -- nodemon -x ts-node src/index.ts",
    "dev:reset": "infisical run --env=dev --path=/Apps/reservation-apps/api --command \"npm run docker:dev -- dev true && npm run migrate && npm run seed && npm run dev\"",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "lint": "npx eslint src/. test/. --config ../../.eslintrc.json",
    "prettier-format": "npx prettier --config ../../.prettierrc \"src/**/*.ts\" \"test/**/*.ts\"  --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --watch --watch-files src/**/*.ts,test/**/*.ts",
    "test": "export NODE_ENV=test; npm run docker:test -- test true && npm run migrate && npm run seed && npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "migrate": "knex migrate:latest && ts-node generate-types.ts && npm run prettier-format",
    "migrate:make": "knex migrate:make",
    "migrate:seed": "knex seed:make",
    "seed": "knex seed:run",
    "migrate:clear": "knex migrate:down",
    "migrate:list": "knex migrate:list",
    "migrate:down": "knex migrate:down",
    "generate:types": "ts-node generate-types.ts",
    "docker:dev": "./docker-init.sh",
    "docker:test": "./docker-init.sh"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@feathersjs/cli": "^5.0.30",
    "@feathersjs/rest-client": "^5.0.30",
    "@infisical/sdk": "^2.3.5",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.4",
    "axios": "^1.7.7",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tsconfig": "*",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@aws-sdk/s3-request-presigner": "^3.673.0",
    "@feathersjs/adapter-commons": "^5.0.30",
    "@feathersjs/authentication": "^5.0.30",
    "@feathersjs/authentication-client": "^5.0.30",
    "@feathersjs/configuration": "^5.0.30",
    "@feathersjs/errors": "^5.0.30",
    "@feathersjs/feathers": "^5.0.30",
    "@feathersjs/knex": "^5.0.30",
    "@feathersjs/koa": "^5.0.30",
    "@feathersjs/schema": "^5.0.30",
    "@feathersjs/socketio": "^5.0.30",
    "@feathersjs/transport-commons": "^5.0.30",
    "@feathersjs/typebox": "^5.0.30",
    "cloudinary": "^2.4.0",
    "feathers-hooks-common": "^8.2.1",
    "knex": "^3.1.0",
    "knex-types": "^0.5.0",
    "pg": "^8.12.0",
    "winston": "^3.14.2"
  }
}
