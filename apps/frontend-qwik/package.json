{
  "name": "frontend-qwik",
  "description": "App with Routing built-in (recommended)",
  "version": "0.0.0",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "wait-on http://localhost:3030 && vite --mode ssr",
    "dev.debug": "wait-on http://localhost:3030 && infisical run --env=dev --path=/Infisical -- node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "test.e2e": "playwright test",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
    "@builder.io/qwik": "1.9.0",
    "@builder.io/qwik-city": "1.9.0",
    "@infisical/sdk": "^3.0.3",
    "@modular-forms/qwik": "^0.27.0",
    "@playwright/test": "^1.35.1",
    "@types/eslint": "8.56.12",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "@typescript-eslint/utils": "^7.16.1",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.14",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "1.9.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.24",
    "prettier": "3.3.3",
    "tailwindcss": "^3.3.2",
    "typescript": "5.4.5",
    "undici": "5.22.1",
    "valibot": "^0.22.0",
    "vite": "5.3.5",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@feathersjs/rest-client": "^5.0.30",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "api": "http://localhost:3030/api-0.0.0.tgz",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "glob": "^10.3.1",
    "zod": "^3.23.8"
  }
}
