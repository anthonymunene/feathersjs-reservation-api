{
  "name": "api",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.9.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "postgresql",
    "framework": "express",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "dev:reset": "npm run docker:dev && npm run migrate && npm run seed && npm run lint && npm run dev",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "lint": "npx eslint src/. test/. --config ../../.eslintrc.json",
    "prettier-format": "npx prettier --config ../../.prettierrc \"src/**/*.ts\" \"test/**/*.ts\"  --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --watch --watch-files src/**/*.ts,test/**/*.ts",
    "test": "export NODE_ENV=test; npm run docker:test && npm run migrate && npm run seed && npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "migrate": "knex migrate:latest && ts-node generate-types.ts",
    "migrate:make": "knex migrate:make",
    "migrate:seed": "knex seed:make",
    "seed": "knex seed:run",
    "migrate:clear": "knex migrate:down",
    "migrate:list": "knex migrate:list",
    "migrate:down": "knex migrate:down",
    "generate:types": "ts-node generate-types.ts",
    "docker:dev": "./docker-init.sh dev true",
    "docker:test": "./docker-init.sh test true"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@feathersjs/cli": "^5.0.4",
    "@feathersjs/rest-client": "^5.0.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.5",
    "axios": "^1.3.4",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "typescript": "^5.0.2",
    "vitest": "^0.30.0"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.5",
    "@feathersjs/authentication": "^5.0.5",
    "@feathersjs/authentication-client": "^5.0.5",
    "@feathersjs/configuration": "^5.0.5",
    "@feathersjs/errors": "^5.0.5",
    "@feathersjs/express": "^5.0.5",
    "@feathersjs/feathers": "^5.0.5",
    "@feathersjs/knex": "^5.0.5",
    "@feathersjs/schema": "^5.0.5",
    "@feathersjs/socketio": "^5.0.5",
    "@feathersjs/transport-commons": "^5.0.5",
    "@feathersjs/typebox": "^5.0.5",
    "feathers-hooks-common": "^7.0.3",
    "knex": "^2.4.2",
    "knex-types": "^0.5.0",
    "pg": "^8.10.0",
    "winston": "^3.8.2"
  }
}
