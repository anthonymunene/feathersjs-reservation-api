{
  "name": "feathers-prisma",
  "description": "feathers prisma playground",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "anthonymunene",
    "email": "munene.anthony@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --fix",
    "dev": "docker-compose down && docker-compose up -d --scale prisma=0 && npm run start ",
    "dev:seed": "docker-compose down && docker-compose up -d && npm run start",
    "debug": "nodemon --inspect src/index.js",
    "start": "nodemon src/index.js",
    "jest": "jest --forceExit",
    "postinstall": "prisma generate"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/configuration": "^4.5.15",
    "@feathersjs/errors": "^4.5.15",
    "@feathersjs/express": "^4.5.15",
    "@feathersjs/feathers": "^4.5.15",
    "@feathersjs/socketio": "^4.5.15",
    "@feathersjs/transport-commons": "^4.5.15",
    "@prisma/client": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-prisma": "^0.5.8",
    "helmet": "^5.1.0",
    "node-fetch": "^3.2.10",
    "serve-favicon": "^2.5.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "axios": "^0.27.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-type": "^17.1.6",
    "file-type-cjs": "^1.0.7",
    "glob": "^8.0.3",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "prisma": "^4.1.0",
    "prisma-dbml-generator": "^0.9.1"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
